blend {
  mode: difference;
  bottom: color {
    bounds: 0 0 100 100;
    color: red;
  }
  top: color {
    bounds: 50 50 100 100;
    color: blue;
  }
}

blend {
  mode: normal;
  bottom: color {
    bounds: 150 0 100 100;
    color: red;
  }
  top: color {
    bounds: 200 50 100 100;
    color: blue;
  }
}
